{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "sb/content-slider",
  "version": "0.1.0",
  "title": "SB Content Slider",
  "category": "sb-blocks",
  "keywords": [
    "solo"
  ],
  "icon": "smiley",
  "description": "Add any content block inside this multipurpose content slider.",
  "supports": {
    "html": false,
    "align": false,
    "sb_uid": true,
    "sb_dimension": false
  },
  "textdomain": "sb_logo",
  "editorScript": "file:./editor.js",
  "editorStyle": "file:./editor.css",
  "script": "file:./frontend.js",
  "style": [
    "wp-sb-frontend",
    "file:./frontend.css"
  ],
  "attributes": {
    "align": {
      "type": "string",
      "default": "center"
    },
    "carouselPerView": {
      "type": "number",
      "default": 1
    },
    "carouselPerViewTablet": {
      "type": "number",
      "default": 1
    },
    "carouselPerViewMobile": {
      "type": "number",
      "default": 1
    },
    "carouselFocusAt": {
      "type": "string"
    },
    "carouselGap": {
      "type": "string",
      "default": "10"
    },
    "carouselAutoplay": {
      "type": "string"
    },
    "carouselAnimationDuration": {
      "type": "string",
      "default": "400"
    },
    "carouselArrows": {
      "type": "boolean",
      "default": true
    },
    "carouselDots": {
      "type": "boolean",
      "default": true
    },
    "verticalAlignment": {
      "type": "string",
      "default": "middle"
    },
    "animationEffect": {
      "type": "string",
      "default": "slide"
    },
    "carouselHeight": {
      "type": "string",
      "default": "400px"
    },
    "carouselHeightTablet": {
      "type": "string"
    },
    "carouselHeightMobile": {
      "type": "string"
    }
  }
}